{
  "url": "ajax/login", //登入
  "type": "GET",
  "params": [
    {"account": "String"}, //用户名
    {"password": "String"} //密码
  ],
  "res": [ 
    {"state": 1, "user": "Object"}, //登入成功
    {"state": 1001 }, //用户名为空
    {"state": 1002 }, //密码为空
    {"state": 1003 }, //用户名不存在
    {"state": 1004 } //密码错误
  ]
},
{
  "url": "ajax/logoff", //注销
  "type": "GET",
  "res": {"state": 1 }  //注销成功
}