    "morgan": "~1.7.0",
    "node-sass": "^4.1.1",
    "sass-loader": "^4.1.1",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
