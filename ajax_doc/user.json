// url：地址, type：ajax类型, param：参数, res：返回值。
{
  "url": "ajax/user/login", //登录
  "type": "GET",
  "param": {
    "account": "String", //用户名
    "password": "String" //密码
  },
  "res": [ 
    {"state": 1, "user": "Object"}, //登录成功
    {"state": 1001 }, //用户名为空
    {"state": 1002 }, //密码为空
    {"state": 1003 }, //用户名不存在
    {"state": 1004 }, //密码错误
    {"state": 2001 }, //你已登录
    {"state": 3001 } //数据库错误
  ]
},
{
  "url": "ajax/user/checkLogin", //检测是否登录
  "type": "GET",
  "param": null,
  "res": [
    {"state": 2001 },  //未登录
    {"state": 2002 } //已登录
  ]
},
{
  "url": "ajax/user/logoff", //注销
  "type": "GET",
  "param": null,
  "res": {"state": 1 }  //注销成功
},
{
