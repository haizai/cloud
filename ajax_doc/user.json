// url：地址, type：ajax类型, param：参数, res：返回值。
{
  "url": "ajax/login", //登入
  "type": "GET",
  "param": {
    "account": "String", //用户名
    "password": "String" //密码
  },
  "res": [ 
    {"state": 1, "user": "Object"}, //登入成功
    {"state": 1001 }, //用户名为空
    {"state": 1002 }, //密码为空
    {"state": 1003 }, //用户名不存在
    {"state": 1004 }, //密码错误
    {"state": 1005 } //数据库错误
  ]
},
{
  "url": "ajax/logoff", //注销
  "type": "GET",
  "param": null,
  "res": {"state": 1 }  //注销成功
},
{
  "url": "ajax/changeSign", //更改个性签名
  "type": "POST",
  "param": {"sign": "String"}, //个性签名
  "res": [
    {"state": 1001 }, //尚未登入
    {"state": 1002 }, //个性签名为空
    {"state": 1003 }, //个性签名超过30个字符
    {"state": 1004 } //数据库错误
  ]  
}