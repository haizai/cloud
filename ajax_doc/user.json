// url：地址, type：ajax类型, param：参数, res：返回值。
{
  "url": "ajax/user/login", //登入
  "type": "GET",
  "param": {
    "account": "String", //用户名
    "password": "String" //密码
  },
  "res": [ 
    {"state": 1, "user": "Object"}, //登入成功
    {"state": 1001 }, //用户名为空
    {"state": 1002 }, //密码为空
    {"state": 1003 }, //用户名不存在
    {"state": 1004 }, //密码错误
    {"state": 3001 } //数据库错误
  ]
},
{
  "url": "ajax/user/logoff", //注销
  "type": "GET",
  "param": null,
  "res": {"state": 1 }  //注销成功
},
{
  "url": "ajax/user/setSign", //更改个性签名
  "type": "POST",
  "param": {"sign": "String"}, //个性签名
  "res": [
    {"state": 1 },
    {"state": 1001 }, //个性签名为空
    {"state": 1002 }, //个性签名超过30个字符
    {"state": 2001 }, //尚未登入
    {"state": 3001 } //数据库错误
  ]  
},
{
  "url":"ajax/user/setSex", //更改性别
  "type": "POST",
  "param": {"sex": "String"},
  "res": [
    {"state": 1 },
    {"state": 1001 }, //性别为空
    {"state": 1002 }, //性别不为给定值
    {"state": 2001 }, //尚未登入
    {"state": 3001 } //数据库错误
  ]  
},
{
  "url":"ajax/user/getProAndCity",//获得省份和城市
  "type":"GET",
  "param":null,
  "res":[
    {"state":1,"proID": "Int","cityID": "Int"},
    {"state": 2001 }, //尚未登入
    {"state": 3001 } //数据库错误
  ]
},
{
  "url":"ajax/user/setProID", //更改省份
  "type": "POST",
  "param": {"proID": "Int"},
  "res": [
    {"state": 1 },
    {"state": 1001 }, //proID为空
    {"state": 1002 }, //proID不全为数字
    {"state": 2001 }, //尚未登入
    {"state": 3001 } //数据库错误
  ]  
},
{
  "url":"ajax/user/setCityID", //更改城市
  "type": "POST",
  "param": {"cityID": "Int"},
  "res": [
    {"state": 1 },
    {"state": 1001 }, //cityID为空
    {"state": 1002 }, //cityID不全为数字
    {"state": 2001 }, //尚未登入
    {"state": 3001 } //数据库错误
  ]  
}